{
  "name": "rocketleaguesam-api-client",
  "version": "0.0.3",
  "description": "Rocket League South America API client",
  "main": "dist",
  "scripts": {
    "watch": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist --ignore node_modules",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "mocha --opts ./mocha.opts",
    "test:watch": "mocha --watch --opts ./mocha.opts",
    "lint": "eslint src test",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "lint"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugogrochau/rocketleaguesam-api-client.git"
  },
  "author": "Hugo Grochau <hugo@grochau.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0"
  },
  "devDependencies": {
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "lint-staged": "^3.2.8",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2"
  },
  "keywords": [
    "south",
    "america",
    "rocket",
    "league",
    "rank",
    "leaderboard"
  ]
}
